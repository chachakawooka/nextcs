{
  "name": "nextcs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "thrift/thrift-0.15.0.exe -r --out thrift --gen js:node,ts,es6,with_ns thrift/api.thrift",
