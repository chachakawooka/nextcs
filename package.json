    "lodash": "^4.17.21",
    "micro": "^9.3.4",
    "moment": "^2.29.1",
    "next": "11.1.3",
    "nextjs-cors": "^2.0.0",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-moment": "^1.1.1",
    "styled-components": "^5.3.3",
    "thrift": "^0.15.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-controls": "^6.3.12",
